<h1><span class="whiteblock">Welcome back, pickler!</span></h1><br><br>

<div class="whiteblock">
<%= form_for :session, url: login_path do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log In", method: :create %>

<% end %>
</div>
